<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/editTextPhoneKris"
        android:layout_width="105dp"
        android:layout_height="57dp"
        android:layout_marginTop="88dp"
        android:ems="10"
        android:inputType="phone"
        android:text="112"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/imhelp"
        android:layout_width="391dp"
        android:layout_height="61dp"
        android:text="Следует вызвать бригаду скорой помощи"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextPhoneKris"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/callToHelp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="88dp"
        android:clickable="true"
        android:contentDescription="@string/call"
        android:onClick="onClickCall"
        app:backgroundTint="#FFAB91"
        app:layout_constraintEnd_toStartOf="@+id/editTextPhoneKris"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/call" />

    <ImageView
        android:id="@+id/imageView154"
        android:layout_width="91dp"
        android:layout_height="106dp"
        android:layout_marginTop="104dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/callToHelp"
        app:srcCompat="@mipmap/window1" />

    <ImageView
        android:id="@+id/imageView554"
        android:layout_width="105dp"
        android:layout_height="127dp"
        android:layout_marginTop="336dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/zipper" />

    <ImageView
        android:id="@+id/imageView77"
        android:layout_width="105dp"
        android:layout_height="131dp"
        android:layout_marginTop="432dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/valley" />

    <ImageView
        android:id="@+id/imageView112"
        android:layout_width="97dp"
        android:layout_height="73dp"
        android:layout_marginTop="176dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.076"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/pill" />

    <TextView
        android:id="@+id/textView2125"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:text="Откройте окно"
        android:textAlignment="center"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toEndOf="@+id/imageView154"
        app:layout_constraintTop_toBottomOf="@+id/textView3578" />

    <TextView
        android:id="@+id/textView3257"
        android:layout_width="244dp"
        android:layout_height="38dp"
        android:layout_marginTop="79dp"
        android:text="Расстегните одежду (ворот и ремень)"
        android:textAlignment="center"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toEndOf="@+id/imageView554"
        app:layout_constraintTop_toBottomOf="@+id/textView2125" />

    <TextView
        android:id="@+id/textView3465"
        android:layout_width="260dp"
        android:layout_height="36dp"
        android:layout_marginTop="48dp"
        android:text="Постарайтесь сохранять спокойствие"
        android:textAlignment="center"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.023"
        app:layout_constraintStart_toEndOf="@+id/imageView77"
        app:layout_constraintTop_toBottomOf="@+id/textView3257" />

    <TextView
        android:id="@+id/textView3578"
        android:layout_width="251dp"
        android:layout_height="76dp"
        android:layout_marginTop="31dp"
        android:text="Разжуйте 1 таблетку ацетилсалициловой кислоты и примите под язык 1 таблетку нитроглицирина"
        android:textAlignment="center"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.258"
        app:layout_constraintStart_toEndOf="@+id/imageView112"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhoneKris" />

    <Button
        android:id="@+id/backToAdAtIM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="372dp"
        android:onClick="onClickBackToAdAtIm"
        android:text="Назад"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView112" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="98dp"
        android:layout_height="71dp"
        android:layout_marginBottom="128dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.063"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/pill" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="244dp"
        android:layout_height="73dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="46dp"
        android:text="Если боли сохраняются, то примите еще 1 таблетку нитроглицирина под язык через минуту. "
        android:textAlignment="center"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:layout_constraintTop_toBottomOf="@+id/textView3465" />

</androidx.constraintlayout.widget.ConstraintLayout>